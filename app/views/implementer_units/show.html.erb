<%- model_class = ImplementerUnit -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @implementer_unit.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @implementer_unit.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:phone) %>:</strong></dt>
  <dd><%= @implementer_unit.phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:facilitator) %>:</strong></dt>
  <dd><%= @implementer_unit.facilitator %></dd>
  <dt><strong><%= model_class.human_attribute_name(:facilitator_phone) %>:</strong></dt>
  <dd><%= @implementer_unit.facilitator_phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:call_center) %>:</strong></dt>
  <dd><%= @implementer_unit.call_center %></dd>
  <dt><strong><%= model_class.human_attribute_name(:subdistrict_id) %>:</strong></dt>
  <dd><%= @implementer_unit.get_subdistrict_name %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              implementer_units_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_implementer_unit_path(@implementer_unit), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              implementer_unit_path(@implementer_unit),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
