<%= simple_form_for [:admin, @subdistrict], :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  <%= f.association :district %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to 'Cancel', subdistricts_path, :class => 'btn' %>
  </div>
<% end %>
